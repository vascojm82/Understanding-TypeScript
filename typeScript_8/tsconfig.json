{
    "compilerOptions": {
        "rootDir": "./",
        "outDir": "./",
        "target": "es5",
        "module": "commonjs",
        "moduleResolution": "node",
        "sourceMap": false,
        "noEmitOnError": true,
        "strict": true,
        "noImplicitAny": true,
        "strictNullChecks": false,
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true
      },
      "exclude":[
        "node_modules"
      ]
}
